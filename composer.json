{
  "name": "gissilali/charts",
  "description": "The missing charting library on laravel, powered by Chartisan from ConsoleTV",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">=7.4",
    "illuminate/contracts": "^7.14.1|^8.0|^9.0",
    "illuminate/http": "^7.14.1|^8.0|^9.0",
    "illuminate/console": "^7.14.1|^8.0|^9.0",
    "illuminate/support": "^7.14.1|^8.0|^9.0",
    "gissilali/chartisan-php": "dev-master"
  },
  "authors": [
    {
      "name": "Èrik C. Forés",
      "email": "soc@erik.cat"
    }
  ],
  "autoload": {
    "psr-4": {
      "ConsoleTVs\\Charts\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ConsoleTVs\\Charts\\ChartsServiceProvider"
      ]
    }
  }
}
